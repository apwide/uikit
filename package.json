{
  "name": "@apwide/uikit",
  "version": "6.0.0",
  "description": "Apwide ui components",
  "main": "dist/uikit.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "gen-stories": "node .storybook/storyGenerator.js",
    "build": "webpack --mode=production --node-env=production",
    "build-dev": "webpack --mode=development --node-env=development",
    "build-storybook": "storybook build -c .storybook -o docs",
    "storybook": "storybook dev -p 9001 -s ./public -c .storybook --ci",
    "lint": "eslint src stories tests --ext .js,.vue,.ts",
    "lint-fix": "eslint src stories tests --fix --ext .js,.vue,.ts",
    "test": "npm run lint && jest && concurrently 'npm run storybook' 'npm run cypress:test' -k -s first",
    "unit": "jest",
    "test:watch": "jest --verbose --watchAll --notify",
    "cypress:test": "wait-on http://localhost:9001 && cypress run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "vue",
    "atlassian"
  ],
  "author": "Apwide",
  "license": "MIT",
  "devDependencies": {
    "@atlaskit/css-reset": "7.3.7",
    "@babel/helper-plugin-utils": "7.27.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@faker-js/faker": "10.0.0",
    "@storybook/addon-actions": "7.6.20",
    "@storybook/addon-docs": "7.6.20",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-links": "7.6.20",
    "@storybook/core-events": "7.6.20",
    "@storybook/vue-webpack5": "7.6.20",
    "@types/dompurify": "3.0.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@vue/cli-plugin-babel": "5.0.9",
    "@vue/eslint-config-standard": "8.0.1",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "1.3.6",
    "@vue/vue2-jest": "29.2.6",
    "assert": "^2.1.0",
    "babel-loader": "10.0.0",
    "babel-preset-typescript-vue": "1.1.1",
    "babel-preset-vue": "2.0.2",
    "concurrently": "9.2.1",
    "core-js": "3.45.1",
    "css-loader": "7.1.2",
    "cypress": "15.3.0",
    "dompurify": "3.2.7",
    "eslint": "8.53.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "9.18.1",
    "file-loader": "6.2.0",
    "html-loader": "5.1.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "lint-staged": "16.2.3",
    "mini-css-extract-plugin": "2.9.4",
    "prettier": "2.8.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "7.6.20",
    "storybook-addon-vue-info": "1.4.2",
    "style-loader": "4.0.0",
    "ts-jest": "29.4.4",
    "ts-loader": "9.5.4",
    "typescript": "5.9.2",
    "typescript-eslint": "0.0.1-alpha.0",
    "vue": "2.7.16",
    "vue-loader": "15.11.1",
    "vue-svg-loader": "0.16.0",
    "vue-template-compiler": "2.7.16",
    "wait-on": "9.0.1",
    "webpack": "5.102.0",
    "webpack-cli": "6.0.1"
  },
  "peerDependencies": {
    "vue": "2.7.x"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "jest --bail --findRelatedTests",
      "npm run lint-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx --no-install lint-staged"
    }
  },
  "dependencies": {
    "@floating-ui/dom": "1.7.4",
    "@fortawesome/fontawesome-svg-core": "7.0.1",
    "@fortawesome/free-brands-svg-icons": "7.0.1",
    "@fortawesome/free-regular-svg-icons": "7.0.1",
    "@fortawesome/free-solid-svg-icons": "7.0.1",
    "@fortawesome/vue-fontawesome": "2.0.10",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "easymde": "2.20.0",
    "vue-promised": "1.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
