{
  "name": "@apwide/uikit",
  "version": "3.22.0",
  "description": "Apwide ui components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "rebuild": "rollup -c",
    "storybook": "start-storybook -p 9001 -s ./public -c .storybook --ci",
    "lint": "eslint src stories tests --ext .js,.vue,.ts",
    "lint-fix": "eslint src stories tests --fix --ext .js,.vue,.ts",
    "test": "npm run lint && jest && concurrently 'npm run storybook' 'npm run cypress:test' -k -s first",
    "unit": "jest",
    "test:watch": "jest --verbose --watchAll --notify",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "cypress:test": "wait-on http://localhost:9001 && cypress run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "prepublishOnly": "npm run build"
  },
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "vue",
    "atlassian"
  ],
  "author": "Adrian Bilicz",
  "license": "MIT",
  "devDependencies": {
    "@atlaskit/css-reset": "4.0.1",
    "@babel/core": "7.17.8",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime-corejs3": "7.17.8",
    "@betit/rollup-plugin-rename-extensions": "0.1.0",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@storybook/vue": "5.3.21",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@vue/eslint-config-standard": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.2.4",
    "babel-preset-vue": "2.0.2",
    "concurrently": "5.3.0",
    "core-js": "3.21.1",
    "cypress": "4.12.1",
    "cypress-plugin-tab": "1.0.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "8.5.0",
    "faker": "4.1.0",
    "file-loader": "4.3.0",
    "html-loader": "0.5.5",
    "husky": "1.3.1",
    "install": "0.13.0",
    "jest": "24.9.0",
    "lint-staged": "12.3.7",
    "p-debounce": "4.0.0",
    "prettier": "^2.7.1",
    "rimraf": "3.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "storybook-addon-vue-info": "1.4.2",
    "vue": "2.7.8",
    "vue-jest": "3.0.7",
    "vue-loader": "15.9.8",
    "vue-svg-loader": "0.16.0",
    "vue-template-compiler": "2.7.8",
    "wait-on": "5.3.0",
    "webpack": "4.46.0"
  },
  "peerDependencies": {
    "vue": "2.7.x"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "jest --bail --findRelatedTests",
      "npm run lint-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@atlaskit/adf-schema": "22.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/vue-fontawesome": "2.0.6",
    "@popperjs/core": "2.11.5",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.3",
    "prosemirror-history": "1.2.0",
    "prosemirror-schema-list": "1.1.6",
    "prosemirror-utils": "0.9.6",
    "prosemirror-view": "1.23.11",
    "tiptap": "1.32.2",
    "tiptap-commands": "1.17.1",
    "vue-promised": "1.2.2"
  },
  "resolutions": {
    "prosemirror-model": "1.8.2"
  }
}
